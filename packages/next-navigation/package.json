{
  "name": "@vooteam/next-navigation",
  "version": "1.0.0",
  "description": "A Next.js navigation hook library for async router operations",
  "author": "Vooteam",
  "license": "MIT",
  "homepage": "https://github.com/vooteam/next-navigation",
  "repository": {
    "type": "git",
    "url": "https://github.com/vooteam/next-navigation.git",
    "directory": "packages/next-navigation"
  },
  "bugs": {
    "url": "https://github.com/vooteam/next-navigation/issues"
  },
  "keywords": [
    "nextjs",
    "navigation",
    "router",
    "react",
    "hook",
    "async",
    "vooteam"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "swc src -d dist --strip-leading-paths --ignore **/*.test.ts,**/*.spec.ts,**/*.test.tsx,**/*.spec.tsx",
    "build:types": "tsc -p tsconfig.lib.json",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "jsdom": "^25.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  }
}
