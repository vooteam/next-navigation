{
  "name": "@vooteam/next-navigation",
  "version": "1.0.0",
  "description": "A powerful, type-safe Next.js navigation library with async router operations, built-in progress indicators, and seamless TypeScript integration.",
  "author": {
    "name": "VooTeam",
    "url": "https://github.com/vooteam"
  },
  "license": "MIT",
  "homepage": "https://github.com/vooteam/next-navigation#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vooteam/next-navigation.git",
    "directory": "packages/next-navigation"
  },
  "bugs": {
    "url": "https://github.com/vooteam/next-navigation/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/vooteam"
  },
  "keywords": [
    "nextjs",
    "next.js",
    "navigation",
    "router",
    "react",
    "hook",
    "async",
    "typescript",
    "progress-bar",
    "loading",
    "app-router",
    "react18",
    "use-transition",
    "type-safe",
    "vooteam"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "!**/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:types && npm run move-files",
    "build:js": "swc src --out-dir dist/temp --config-file .swcrc",
    "build:types": "tsc",
    "move-files": "cp dist/temp/src/index.* dist/ && mkdir -p dist/lib && cp dist/temp/src/lib/*.js dist/lib/ && cp dist/temp/src/lib/*.js.map dist/lib/ && rm -rf dist/temp && find dist/lib -name '*.spec.*' -delete",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": false
    },
    "react": {
      "optional": false
    }
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^25.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false
}
