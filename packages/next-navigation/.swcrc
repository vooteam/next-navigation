{
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic"
      }
    },
    "keepClassNames": true,
    "externalHelpers": false
  },
  "module": {
    "type": "commonjs",
    "strict": true
  },
  "sourceMaps": true
}
